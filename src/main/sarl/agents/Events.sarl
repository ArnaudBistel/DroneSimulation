package agents

import gui.DroneBody
import java.util.UUID
import gui.Body
import java.util.List
import map.MapPoint
import java.util.ArrayList
import map.MapPointType

event NewDrone {
	var droneBody : DroneBody
	new (drone : DroneBody) {
		this.droneBody = drone
	}
	
}

event DronePerception {
	var id : UUID
	var perceived : List<Body>
	var step : int

	new (id : UUID, perceived : List<Body>, step : int) {
		this.id = id
		this.perceived = perceived
		this.step = step
	}

}

event ActionEvent {
	var action : Action

	new (action : Action) {
		this.action = action
	}

}

event SendDeliveryList {
	var deliveryList : ArrayList<List<MapPoint>>

	new (deliveryList : ArrayList<List<MapPoint>>) {
		this.deliveryList = deliveryList
	}
}

event DronePathAssignment {
	var path : List<MapPoint>

	new (path : List<MapPoint>) {
		this.path = path
	}
}

event MakeAssignmentPaths {
	new () {
	}
}

event Restart {
	new (){
		
	}
}

event ImportDataEvent{
	var filename : String
	new(name : String) {
		this.filename = name
	}
}

event ExportDataEvent {
	var filename : String

	new(name : String) {
		this.filename = name
	}
}

event AgentInitialized {
	var type : AgentType
	
	new (type : AgentType) {
		this.type = type
	}
	
}

